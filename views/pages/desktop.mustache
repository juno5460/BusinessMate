{{> page.companyreport}}

<div class="hr hr32 hr-dotted" style="margin-top:60px"></div>
<div class="row-fluid">
	<div class="span12">
		<h3 class="header smaller blue">
				回款统计
		</h3>
		<div id="table-header">
			<h4 class="header smaller lighter blue">
				已回款汇总
			</h4>
		</div>
		<table id="table_bug_report" class="table table-striped table-bordered table-hover">
			<thead>
				<tr>
					<th>合同名</th>
